.lineBox {
  width: 100%;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
}
.inlineBox {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.fullLine {
  width: 100%;
}
.spaceBox {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.borderBox {
  border: 1rpx solid lightgray;
  border-radius: 10rpx;
  margin-bottom: 1rpx;
}
.rightBox {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.fillBox {
  flex: 1 1 auto;
}
.bottomLine {
  width: 100%;
  border-bottom: 1rpx solid lightgray;
}
.topSpace {
  margin-top: 20rpx;
}
.rbTip {
  position: absolute;
  bottom: 0;
  right: 0;
  background-color: lightgray;
  font-size: 24rpx;
  border-top-left-radius: 5rpx;
  padding: 0rpx 3rpx;
  vertical-align: bottom;
}
.miniText {
  font-size: 18rpx;
  line-height: 20rpx;
}
.labelText {
  font-size: 28rpx;
  padding-left:5rpx;
}
.inline {
  display: inline-block;
}
.R {
  color:blue;
}
.SR {
  color:blueviolet;
}
.SSR {
  color:coral;
}
.bgSR {
  background-color: rgb(214, 169, 255);
}
.bgSSR {
  background-color: rgb(255, 212, 131);
}
.red {
  color: red;
}
.blue {
  color: blue;
}
.purple {
  color: blueviolet;
}
.orange {
  color: coral;
}
.green {
  color: green;
}
.gray {
  color: darkgray;
}
.black {
  color: black;
}
.grayLine {
  background-color: rgb(235,235,235);
}
.tipText {
  font-size: 20rpx;
  padding:0 5rpx;
  color: darkgray;
}