@import "./common.wxss";
.title {
  font-size: 40rpx;
  display: inline-block;
  vertical-align: bottom;
  margin: 2rpx 5rpx;
}
.tipTitle {
  font-size: 28rpx;
  margin-left: 10rpx;
}
.topButton {
  display: inline-block;
  margin: 2rpx 5rpx;
}
.thank {
  width: 100%;
  text-align: center;
  font-size: 32rpx;
}
.logText {
  font-size: 24rpx;
  margin: 5rpx 10rpx;
}
.scoreBox {
  margin: 10rpx 10rpx;
  width: 600rpx;
}
.scoreText {
  padding: 5rpx 10rpx;
  font-size: 28rpx;
}
.score {
  margin: 5rpx 0rpx;
  font-size: 36rpx;
}
.midBox {
  display: flex;
  flex-direction: column;
  width: 100%;
  align-items: center;
}
.menuBox {
  width: 300rpx;
  border-radius: 10rpx;
  background-color: white;
  border: 1rpx solid gray;
  overflow: hidden;
  text-align: center;
}
.menu {
  font-size: 40rpx;
  padding: 10rpx;
}