@import "../common.wxss";
.buff-box {
  padding: 5rpx 10rpx;
}
.grey {
  background-color: rgb(238, 238, 238);
}
.name {
  display: flex;
  font-size: 28rpx;
}
.name-container {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.activated {
  color:black;
}
.inactive {
  color:gray;
}
.desc {
  font-size: 18rpx;
}
.inputBox {
  border: 1rpx solid darkgray;
  border-radius: 5rpx;
  width: 80rpx;
  text-align: center;
  background-color: white;
}