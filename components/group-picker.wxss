.top {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 36px;
  border-bottom: 1rpx solid #d3cfcf;
}

.top-text {
  font-size: 20px;
  width: 80px;
  height: 24px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.item {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.content {
  display: flex;
  flex-direction: column;
  width: 100%;
  background: white;
}

.R {
  color:blue;
}
.SR {
  color:blueviolet;
}
.SSR {
  color:coral;
}
.INVALID {
  color:darkgray;
}