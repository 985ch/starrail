@import "../common.wxss";
.itemBox {
  display: inline-block;
  vertical-align: middle;
  height: 74rpx;
  width: 74rpx;
  border: 1rpx solid lightgray;
  border-radius: 10rpx;
  position: relative;
  margin: 1rpx 1rpx;
  overflow: hidden;
}
.locked {
  border-color: darkgray;
}
.selected {
  border-color: red;
}
.iconImg {
  width: 100%;
  height: 100%;
}
.tagLevel {
  position: absolute;
  top: 0;
  right: 0;
  background-color: lightgray;
  font-size: 16rpx;
  border-bottom-left-radius: 8rpx;
  padding: 0rpx 2rpx;
}
.tagMain {
  position: absolute;
  bottom: 0;
  right: 0;
  background-color: lightgray;
  font-size: 20rpx;
  border-top-left-radius: 8rpx;
  padding: 0rpx 2rpx;
}
.tagLock {
  position: absolute;
  bottom: 0;
  left: 0;
  background-color: lightgray;
  font-size: 20rpx;
  border-top-right-radius: 8rpx;
  padding: 0 5rpx;
}