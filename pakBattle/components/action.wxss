@import "../../components/common.wxss";
.container {
  width: 745rpx;
  height: 60rpx;
}
.line {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
}
.bgWhite {
  background-color: white;
}
.bgGray {
  background-color: rgb(235, 235, 235);
}
.iconBox {
  position: relative;
  display: inline-block;
  vertical-align: bottom;
  width: 55rpx;
  height: 55rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.icon {
  width: 100%;
  height: 100%;
}
.iconIdx {
  position: absolute;
  top: 0;
  right: 2rpx;
  color: white;
  font-size: 28rpx;
  padding: 0rpx 4rpx;
}
.index {
  width: 61rpx;
  font-size: 24rpx;
  text-align: center;
}
.act {
  font-size: 28rpx;
  margin: 0 5rpx;
}
.leftSpace {
  margin-left: 5rpx;
}
.rightSpace {
  margin-right: 5rpx;
}
.halfLine {
  width:342rpx;
}
.rightText {
  text-align: right;
}
.spBox {
  line-height: 12rpx;
}
.spIcon {
  width: 12rpx;
  height: 12rpx;
}